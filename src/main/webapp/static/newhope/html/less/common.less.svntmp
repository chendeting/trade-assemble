//公共属性
*{
  padding:0;
  margin:0;
  font-family: "Microsoft YaHei";
  //background-color: #fff;
  font-size: 0.6666rem;
}
body{
  font-family: "Microsoft YaHei";
  //&::-webkit-scrollbar{
  //  width:0;
  //  height:0;
  //}
}
#warp {
  width: 100%;
  height: 100%;
  //background-color: #eee;
}
//html,body{
//  height:100%;
//  //overflow:hidden;
//  overflow-y:hidden;
