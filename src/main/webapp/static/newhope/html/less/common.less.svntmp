p{
  margin:0;
  line-height:1rem;
}
ul li{
  list-style: none;
  //display: inline-block;
}
a{
  color:#000;
  text-decoration: none;
}

input,textarea{
  outline: none;
  background-color: #d8d8d8;
  border-radius: 0.15625rem;
  border-style: none;
}
button{
  font-size: 0.7777rem;
  border-style: none;
  border-radius: 0.15625rem;
  color: #fff;
}
.clearfloat{
  clear: both;
}
